@article{ ISI:000391040100001,
Author = {Samuels, J. Troy},
Title = {{A CAMEL DRIVER IN THE ACHAEMENID PERSIAN EMPIRE: SOCIAL NETWORK ANALYSIS
   OF A NON-ELITE PROFESSIONAL AND THE PERSEPOLIS FORTIFICATION ARCHIVE}},
Journal = {{BULLETIN OF THE INSTITUTE OF CLASSICAL STUDIES}},
Year = {{2016}},
Volume = {{59}},

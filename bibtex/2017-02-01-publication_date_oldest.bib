@article{ ISI:000223555600077,
Author = {Motter, AE},
Title = {{Cascade control and defense in complex networks}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2004}},
Volume = {{93}},
Number = {{9}},
Month = {{AUG 27}},
Abstract = {{Complex networks with a heterogeneous distribution of loads may undergo
   a global cascade of overload failures when highly loaded nodes or edges
   are removed due to attacks or failures. Since a small attack or failure
   has the potential to trigger a global cascade, a fundamental question
   regards the possible strategies of defense to prevent the cascade from

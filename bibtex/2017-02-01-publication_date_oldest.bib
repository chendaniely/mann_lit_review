@article{ ISI:000182127200022,
Author = {Moreno, Y and Pastor-Satorras, R and Vazquez, A and Vespignani, A},
Title = {{Critical load and congestion instabilities in scale-free networks}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{2003}},
Volume = {{62}},
Number = {{2}},
Pages = {{292-298}},
Month = {{APR}},
Abstract = {{We study the tolerance to congestion failures in communication networks
   with scale-free topology. The traffic load carried by each damaged
   element in the network must be partly or totally redistributed among the
   remaining elements. Overloaded elements might fail on their turn,
   triggering the occurrence of failure cascades able to isolate large
   parts of the network. We find a critical traffic load above which the
   probability of massive traffic congestions destroying the network
   communication capabilities is finite.}},
